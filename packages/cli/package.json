  },
  "scripts": {
    "lint": "eslint src test",
    "build:mjs": "tsc --module esnext --outDir ./bin",
    "build": "npm run build:mjs",
    "pretest": "npm run build",
    "test:raw": "node --loader ts-node/esm test/index.ts",
